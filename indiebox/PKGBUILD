pkgname=indiebox
pkgver=0.1
pkgrel=1
pkgdesc="application management framework"
arch=('any')
url="http://indieboxproject.org/"
license=('GPL')
groups=()
backup=()
source=(README)
md5sums=('48b4847cf44d760d3916a64e9de1f8a9')

build() {
    mkdir -p $pkgdir/usr/share/doc/indiebox
    install -m644 $srcdir/README $pkgdir/usr/share/doc/indiebox
}
